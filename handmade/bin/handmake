#!/usr/bin/python
if __name__ == '__main__':
    # EASY-INSTALL-ENTRY-SCRIPT: 'invoke==0.9.0','console_scripts','invoke'
    __requires__ = 'invoke==0.9.0'
    import sys
    from pkg_resources import load_entry_point

    if __name__ == '__main__':
        sys.exit(
            load_entry_point('invoke==0.9.0', 'console_scripts', 'invoke')()
        )
